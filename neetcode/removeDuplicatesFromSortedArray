class Solution {
    public int removeDuplicates(int[] nums) {

        int left = 0;
        int right = 0;

        while (right < nums.length){
            nums[left] = nums[right];
            
            while (right < nums.length && nums[left]==nums[right]){
               right+=1; 
            }
            left+=1;

            

        }
        return left;
        
    }
}